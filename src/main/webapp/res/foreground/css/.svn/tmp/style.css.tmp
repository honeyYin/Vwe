.rolinList{ width:202px;margin:0px auto 0 auto; text-align:left}
.rolinList li{margin-bottom:1px; border-bottom:1px solid #DADADA}
.rolinList li h2{ 
	width:180px;
    font-size:12px;
   padding-left:20px; 
   color:#036;
    cursor:pointer; 
	padding-top:0;
	 margin-bottom:0px;}
.service_list{ width:200px;   padding-bottom:7px;}
.service_list p{ margin:12px}

#service_nav a{float:left; padding-left:24px; width:200px; border-bottom:1px dotted #E3E3E3; margin-top:3px;}
#service_nav a:hover{padding-left:27px;}
#service_right li{
	width:600px;
	margin-top:5px;
}


/*  ========================================================    公共服务      ==============================================================  */
#service_box table td{ width:350px; padding-top:30px; padding-right:60px;
}

#service_box table {margin-left:50px; margin-bottom:50px;}

#service_box h3{
	padding-bottom:5px;
	width:220px; border-bottom:1px solid #CCC;}
	
#service_box img{
	float:right; margin-right:20px;}
	
#service_box ul{
	padding-right:5px;
	float:left;
	margin-left:25px;
	margin-right:30px;
	margin-top:8px;
	list-style-image:url(../images/list_icon.png);
	}
	
#service_box a:hover{ border-bottom:1px solid #036;}


/*  ========================================================    网上办事    ==============================================================  */


#work_left {
	background-image:url(../images/bg_left.png);
	background-repeat:repeat-y;
	width:233px;
	padding-bottom:20px;
	float:left;
	display:inline;
	}
	
#work_left h3{
	border-bottom:1px solid #CCC;
	padding-bottom:6px;
	width:210px;
	color:#036;
}


#work_left form{margin-left:20px;}
#work_left  a:hover{
	color:#036;
	text-decoration:underline;}
 a{
	color:#555;}
	
#work_left ul{
	padding-left:20px;}
#work_left .tabsBox{ width:220px; margin:5px 0;}
#work_left .tabs{ height:15px;}
#work_left .tabs li{ width:80px; text-align:center; padding-top:2px; line-height:18px; float:left; background-color:#ccc; margin-right:5px; list-style:none; position:relative; cursor:pointer; color:#333;}
#work_left .tabs li span{ display:none;}
#work_left .tabs .cur{ background-color:#f0f0f0;}
#work_left .tabs .cur span{ display:block; width:1px; height:1px; border:solid 8px; border-color:#f0f0f0 #fff #fff #fff; position:absolute; top:20px; left:32px; overflow:hidden; z-index:-1;}
#work_left .cons{ height:auto; padding:8px 5px; overflow:hidden; border:1px solid #f0f0f0; position:relative; z-index:1; margin-top:10px}
#work_left .cons ul li{ list-style:none; line-height:20px;}
#work_left .hidden{ display:none;}
#work_left .block{ display:block;}

.work_right{
	 border-bottom:1px solid #CCC;
	 float:left; width:650px; 
	 margin-left:40px;
	 margin-top:10px;
     margin-bottom:0;
	    padding-right:20px;
		display:inline;}
.work_right img{display:inline;}
.work_right ul{ margin-left:-20px; width:570px;_width:500px; list-style-image:url(../images/list_icon.png); margin-top:10px;}
.work_right ul li{margin:10px 45px; float:left; display:inline;}